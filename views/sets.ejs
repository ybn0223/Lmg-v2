<!doctype html>
<html lang="nl">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Blacklist</title>
    <%- include("partials/linksAndScripts") %>
</head>
<body>
<header>
    <%- include("partials/navbar") %>
</header>
<main>
    <!-- profile modal -->
    <!-- Profile modal code remains unchanged -->
    <h1 class="text-center mx-2"> Sets </h1>
    <article class="container d-flex flex-row flex-wrap justify-content-between">
        <% setsShow.forEach((set, index) => { %>
            <section class="card m-3 shadow-sm text-center">
                <img width="100%" height="225" src="<%= set.set_img_url %>">
                <h3 class="border border-warning m-1"><%= set.name %></h3>
            </section>
        <% }) %>
    </article>
</main>
<footer class="py-3 mt-5">
    <%- include("partials/footer") %>
</footer>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>